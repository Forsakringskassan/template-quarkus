{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "local>Forsakringskassan/renovate-config"
  ],
  "packageRules": [
    {
      "description": "Internal FK Maven packages",
      "matchDatasources": [
        "maven"
      ],
      "registryUrls": [
        "https://maven.pkg.github.com/Forsakringskassan/*"
      ],
      "matchPackageNames": [
        "se.fk.{/,}**"
      ]
    }
  ]
}
