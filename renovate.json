{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "local>Forsakringskassan/renovate-config"
  ],
    "hostRules": [
        {
            "hostType": "maven",
            "matchHost": "maven.pkg.github.com",
            "username": "${PACKAGES_RW_ACTOR}",
            "password": "${PACKAGES_RW_TOKEN}"
        }
    ],
  "packageRules": [
    {
      "description": "Internal FK Maven packages",
      "matchDatasources": [
        "maven"
      ],
      "registryUrls": [
        "https://maven.pkg.github.com/Forsakringskassan/*"
      ],
      "matchPackageNames": [
        "se.fk.{/,}**"
      ]
    }
  ]
}
